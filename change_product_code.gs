function changeProductCode(num) {
  var product = "";
  if (num == "7") product = "GV New";
  else if (num == "8") product = "GTa";
  else if (num == "9") product = "GTe";
  else if (num == "1") product = "GV Old";
  else if (num == "2") product = "GT";
  else if (num == "5") product = "GE";

  return product;
}
